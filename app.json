{
  "expo": {
    "name": "Control de Tareas",
    "slug": "control-de-tareas-v2-fixed",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/task_logo_final.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ville.controldetareas",
      "adaptiveIcon": {
        "foregroundImage": "./assets/task_logo_final.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tus fotos para adjuntar evidencia de tus tareas.",
          "cameraPermission": "La app necesita acceso a tu c√°mara para tomar fotos de tus tareas completadas."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b13f414c-340a-4f37-81a2-25f32c77d98c"
      }
    },
    "owner": "ville545"
  }
}
